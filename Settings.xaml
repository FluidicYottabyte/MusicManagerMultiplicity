<Window x:Class="MusicManagerMultiplicity.Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MusicManagerMultiplicity"
        mc:Ignorable="d"
        Title="Settings" Height="150" Width="200">
    <Grid>
        <StackPanel>
            <TextBlock TextWrapping="Wrap">Run song maintenance (time depends on library size)</TextBlock>
            <Button Width="100" Click="ReloadSongs">Run</Button>
        </StackPanel>
        
    </Grid>
</Window>
